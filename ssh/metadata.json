{
  "name": "spiette-ssh",
  "version": "0.4.2",
  "author": "Simon Piette <piette.simon@gmail.com>",
  "summary": "openssh server and client configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/spiette/puppet-ssh",
  "project_page": "https://github.com/spiette/puppet-ssh",
